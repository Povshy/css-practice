<main class="row ms-2">
    {{!-- ----------nav-------------- --}}
    <div class="py-3">
        {{#with navigation}}
        <div class="card">
            <div class="nav">
                {{#each links}}
                <div>
                    {{#if href}}
                    <a href="{{link}}" class="nav-link">{{text}}</a>
                    {{else}}
                    <a href="{{link}}" class="nav-link disabled">{{text}}</a>
                    {{/if}}
                </div>
                {{/each}}
            </div>

        </div>
        {{/with}}
    </div>
    {{!-- ---------------------------- --}}
    <h2>{{header}}</h2>
    <div class="col-12">
        <div class="row g-2">
            {{!-- ============= --}}
            <div class="col-8">
                {{#with contacts_data}}
                <h3>{{title}}</h3>
                {{!-- ========== --}}
                <div class="my-3">
                    {{#each buttons}}
                    {{#if isPrimary}}
                    <button class="btn btn-warning">{{text}}</button>
                    {{else}}
                    <button class="btn btn-outline-warning">{{text}}</button>
                    {{/if}}
                    {{/each}}
                </div>
                {{!-- ========= --}}
                <div>
                    {{#with form}}
                    <div class="row">
                        {{#with input}}
                        <div class="col-6">
                            {{#with surname}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                            {{/with}}
                        </div>

                        <div class="col-6">
                            {{#with name}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                            {{/with}}
                        </div>

                        <div class="col-6">
                            {{#with phone}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                            {{/with}}
                        </div>

                        <div class="col-6">
                            {{#with email}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                            {{/with}}
                        </div>


                        {{/with}}
                    </div>
                    <hr>

                    <div>
                        {{#each switch}}
                        <div class="form-check form-switch">
                            <label class="form-check-label">{{text}}</label>
                            {{#if isActive}}
                            <input type="checkbox" class="form-check-input" checked>
                            {{else}}
                            <input type="checkbox" class="form-check-input">
                            {{/if}}
                        </div>
                        {{/each}}
                    </div>




                    {{/with}}
                </div>

                {{/with}}

                <hr>

            </div>
            {{!-- ============= --}}

            <div class="col-4">
                <div class="card">
                    <div class="card-body">
                        {{#each action_block}}
                        <div class="mb-2">
                            {{#if isSuccess}}
                            <button class="btn btn-success">{{text}}</button>
                            {{/if}}
                            {{#if isDanger}}
                            <button class="btn btn-danger">{{text}}</button>
                            {{/if}}
                            {{#if isInfo}}
                            <button class="btn btn-info">{{text}}</button>
                            {{/if}}
                        </div>
                        {{/each}}
                    </div>
                </div>

                <div class="card my-3">
                    <div class="card-body">
                        {{#with total}}
                        <h3>{{title}}</h3>
                        <div>
                            {{#each item_list}}
                            <div>{{name}}: {{price}}</div>
                            {{/each}}
                        </div>
                        <hr>
                        <div>
                            {{#each additional_info}}
                            <div>{{text}}: {{value}}</div>
                            {{/each}}
                        </div>
                        <hr>
                        <div>
                            {{#with total_price}}
                            <div>{{text}}: {{value}}</div>
                            {{/with}}
                        </div>
                        <hr>
                        <div>
                            {{#each button}}
                            <div class="mb-2">
                                {{#if isPrimary}}
                                <button class="btn btn-primary">{{text}}</button>
                                {{/if}}
                                {{#if isOutline}}
                                <button class="btn btn-outline-primary">{{text}}</button>
                                {{/if}}
                            </div>
                            {{/each}}
                        </div>


                        {{/with}}
                    </div>
                </div>

            </div>


            {{!-- ---------------------------- --}}

            <div class="col-8">
                <div>
                    {{#with delivery}}
                    <h3>{{title}}</h3>
                    <div class="card">
                        <div class="card-body row">
                            {{#each select}}
                            <div class="col-6">
                                <div class="form-label">{{label}}</div>
                                <div>
                                    <select class="form-select" placeholder="{{placeholder}}">
                                        {{#each options}}
                                        <option name="city" value="{{value}}">{{text}}
                                        </option>
                                        {{/each}}
                                    </select>
                                </div>

                            </div>
                            {{/each}}
                        </div>
                    </div>
                    {{!-- ============== --}}

                    <div class="my-3">
                        {{#each delivery_type}}
                        <div class="form-check">
                            <label class="form-check-label">{{type}}</label>
                            {{#if @first}}
                            <input name="delivery" type="radio" class="form-check-input" checked>
                            {{else}}
                            <input name="delivery" type="radio" class="form-check-input">
                            {{/if}}
                        </div>
                        {{/each}}
                        <hr>
                    </div>
                    {{!-- ============== --}}
                    <div>
                        {{#with payment}}
                        <h3>{{title}}</h3>
                        <div>
                            {{#each list}}
                            <div class="form-check my-3">
                                <label class="form-check-label">{{type}}</label>
                                {{#if @first}}
                                <input name="delivery" type="radio" class="form-check-input" checked>
                                <div class="form-text">{{description}}</div>
                                {{else}}
                                <input name="delivery" type="radio" class="form-check-input">
                                {{/if}}
                            </div>
                            {{/each}}
                        </div>
                        <hr>
                        {{/with}}
                    </div>
                    {{!-- ============== --}}
                    <div>
                        {{#with contact}}
                        <h3>{{title}}</h3>
                        <div class="row">
                            <div class="col-6">
                                {{#with surname}}
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" placeholder="{{placeholder}}">
                                <div class="form-text">{{caption}}</div>
                                {{/with}}
                            </div>

                            <div class="col-6">
                                {{#with name}}
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" placeholder="{{placeholder}}">
                                <div class="form-text">{{caption}}</div>
                                {{/with}}
                            </div>

                            <div class="col-6">
                                {{#with phone}}
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" placeholder="{{placeholder}}">
                                <div class="form-text">{{caption}}</div>
                                {{/with}}
                            </div>

                            <div class="col-6">
                                {{#with email}}
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" placeholder="{{placeholder}}">
                                <div class="form-text">{{caption}}</div>
                                {{/with}}
                            </div>
                        </div>
                        {{/with}}
                        <hr>
                    </div>
                    {{!-- ============== --}}
                    <div>
                        {{#with coment}}
                        <label class="form-label">{{label}}</label>
                        <input class="form-control" placeholder="{{placeholder}}">
                        {{/with}}
                        <hr>
                    </div>
                    {{!-- ============== --}}
                    {{/with}}
                    {{!-- ============== --}}

                    <div>
                        {{#with coupon}}
                        <h3>{{title}}</h3>
                        <div>
                            {{#with select}}
                            <div class="row">
                                <div class="my-3">
                                    {{#with availabel_coupon}}
                                    <div class="form-label my-3">{{label}}</div>
                                    <div>
                                        <select class="form-select-lg col-12">
                                            {{#each options}}
                                            <option name="coupon" value="{{value}}">{{text}}
                                            </option>
                                            {{/each}}
                                        </select>
                                    </div>
                                    {{/with}}
                                </div>

                                <div class="my-3">
                                    {{#with friend_coupon}}
                                    <div class="form-label">{{label}}</div>
                                    <div>
                                        <select class="form-select-sm col-12">
                                            {{#each options}}
                                            <option name="coupon" value="{{value}}">{{text}}
                                            </option>
                                            <div class="form-text">{{caption}}</div>
                                            {{/each}}
                                        </select>
                                    </div>
                                    {{/with}}
                                </div>
                            </div>
                            {{/with}}

                            {{#with add}}
                            <div class="row">
                                <div class="my-3">
                                    {{#with friend}}
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control-lg col-12" placeholder="{{placeholder}}">
                                    <div class="form-text">{{caption}}</div>
                                    {{/with}}
                                </div>
                            </div>

                            <div class="row">
                                <div class="my-3">
                                    {{#with colleague}}
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control-sm col-10" placeholder="{{placeholder}}">
                                    <div class="form-text">{{caption}}</div>
                                    {{/with}}
                                </div>
                            </div>
                            {{/with}}

                        </div>
                        <hr>
                        {{/with}}
                    </div>
                    {{!-- ============== --}}

                    <div>
                        {{#with quiz}}
                        <h3>{{title}}</h3>
                        <div>
                            {{#with preference}}
                            <div>{{title}}</div>
                            {{#each options}}
                            <div class="form-check">

                                <label class="form-check-label">{{this}}</label>
                                {{#if @first}}
                                <input name="preference" type="radio" class="form-check-input" checked>
                                {{else}}
                                <input name="preference" type="radio" class="form-check-input">
                                {{/if}}

                            </div>
                            {{/each}}
                            {{/with}}
                        </div>
                        {{!-- ============ --}}

                        <div class="mt-3">
                            {{#with your_expirience}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control">
                            {{/with}}
                        </div>
                        {{!-- ==== --}}

                        <div class="my-3">
                            {{#with purchase_frequency}}
                            <div class="form-label my-3">{{label}}</div>
                            <div>
                                <select class="form-select-lg col-12">
                                    {{#each options}}
                                    <option name="coupon" value="{{value}}">{{text}}
                                    </option>
                                    {{/each}}
                                </select>
                            </div>
                            {{/with}}
                        </div>
                        {{!-- ====== --}}
                        <div class="mb-5">
                            {{#with factors}}
                            <div>{{title}}</div>
                            {{#each options}}
                            <div class="form-check">

                                <label class="form-check-label">{{this}}</label>
                                {{#if @first}}
                                <input name="preference" type="checkbox" class="form-check-input" checked>
                                {{else}}
                                <input name="preference" type="checkbox" class="form-check-input">
                                {{/if}}

                            </div>
                            {{/each}}
                            {{/with}}
                        </div>

                        {{!-- ------------------ --}}



                        {{/with}}
                    </div>
                </div>

            </div>
            {{!-- ========================== --}}


        </div>
    </div>
    <hr>




    {{!-- --------foooter---------- --}}

    <div class="col-12">
        <div class="row">
            {{#each footer}}
            <nav class="col-2">
                <div class="nav">
                    <div class="nav-item">
                        {{#each this}}
                        <div>
                            {{#if @last}}
                            <a href="{{link}}" class="nav-link disabled">{{text}}</a>
                            {{else}}
                            <a href="{{link}}" class="nav-link">{{text}}</a>
                            {{/if}}
                        </div>
                        {{/each}}
                    </div>
                </div>
            </nav>
            {{/each}}
        </div>

    </div>
    {{!-- --------------------- --}}


</main>